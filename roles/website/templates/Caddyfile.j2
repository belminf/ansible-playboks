{{ site_name }} {
    root /var/www/html
}
{% if redirects is defined %}

{{ redirects }} {
    redir https://{{ site_name }}
}
{% endif %}
{% if proxies is defined %}
{% for this_proxy in proxies %}

{{ this_proxy['from'] }} {
  proxy / {{ this_proxy['to'] }}{% if this_proxy['transparent'] %} {
    transparent
  }{% endif %}

}
{% endfor %}
{% endif %}

